include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
        int n, a;
    scanf("%d", &n);
    while(n>0)
    {
        a=n%10;
        printf("%d ",a);
        n=n/10;
    
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
